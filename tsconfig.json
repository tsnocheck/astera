{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "declaration": true,
    "declarationMap": true,
    "baseUrl": ".",
    "paths": {
      "@lolz-bots/shared": [
        "libs/shared/src/index.ts"
      ],
      "@lolz-bots/shared/*": [
        "libs/shared/src/*"
      ]
    },
    "lib": [
      "ESNext"
    ],
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization": false
  },
  "files": [],
  "references": [
    {
      "path": "./apps/ticket-bot"
    },
    {
      "path": "./apps/backup-bot"
    },
    {
      "path": "./apps/utils-bot"
    },
    {
      "path": "./apps/sup-bot"
    },
    {
      "path": "./libs/shared"
    }
  ]
}
